{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "assets/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/register",
      "dest": "/server.js"
    },
    {
      "src": "/login",
      "dest": "/server.js"
    },
    {
      "src": "/verify",
      "dest": "/server.js"
    },
    {
      "src": "/profile",
      "dest": "/server.js"
    },
    {
      "src": "/users/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/farm1(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/farm2(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/farm3(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/farm4(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/settings",
      "dest": "/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "cleanUrls": true
}
